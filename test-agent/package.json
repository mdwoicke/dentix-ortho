{
  "name": "test-agent",
  "version": "1.0.0",
  "description": "E2E Testing Agent for Flowise Dental Appointment Chatbot",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "run": "ts-node src/index.ts run",
    "run:happy": "ts-node src/index.ts run --category happy-path",
    "run:failed": "ts-node src/index.ts run --failed",
    "results": "ts-node src/index.ts results --last",
    "recommendations": "ts-node src/index.ts recommendations",
    "report": "ts-node src/index.ts report --format markdown",
    "diagnose": "ts-node src/index.ts diagnose --category happy-path",
    "diagnose:all": "ts-node src/index.ts diagnose",
    "diagnose:no-llm": "ts-node src/index.ts diagnose --category happy-path --no-llm",
    "fixes": "ts-node src/index.ts fixes",
    "fixes:prompt": "ts-node src/index.ts fixes --type prompt",
    "fixes:tool": "ts-node src/index.ts fixes --type tool",
    "fix-report": "ts-node src/index.ts fix-report",
    "fix-status": "ts-node src/index.ts fix-status",
    "analyze": "ts-node src/index.ts analyze",
    "llm-prompt": "ts-node src/llm-prompt.ts",
    "build": "tsc",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@faker-js/faker": "^10.1.0",
    "axios": "^1.6.7",
    "better-sqlite3": "^9.4.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^17.2.3",
    "ioredis": "^5.9.2",
    "langfuse": "^3.38.6",
    "uuid": "^9.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
